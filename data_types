import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int i; double d; String s;
        Scanner sc = new Scanner(System.in);
        i=sc.nextInt();
        d=sc.nextDouble();
        sc.nextLine();
        s=sc.nextLine();
        System.out.println(i+(int)d);
        System.out.println(2*d);
        System.out.println("HackerRank "+s);
        sc.close();
    }
}
